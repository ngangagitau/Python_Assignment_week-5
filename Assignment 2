class Vehicle:
    def move(self):
        print("This vehicle moves in a generic way.")

class Car(Vehicle):
    def move(self):
        print("Driving on the road! ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Flying through the skies! âœˆï¸")

class Boat(Vehicle):
    def move(self):
        print("Sailing across the water! â›µ")

def travel(vehicle):
    vehicle.move()

my_car = Car()
my_plane = Plane()
my_boat = Boat()

travel(my_car)
travel(my_plane)
travel(my_boat)
